        <item name="colorTitle">@style/backgrp5</item>
        <item name="colorTitle2">@style/bat5</item>
        <item name="mtextc">@style/textc5</item>
    </style>
    <style name="backgrp">
        <item name="android:background">@color/c1_1</item>
    </style>
    <style name="textc">
        <item name="android:textColor">@color/c1_2</item>
    </style>
    <style name="bat">
        <item name="android:background">@drawable/botton_selector</item>
    </style>
